<?xml version="1.0" encoding="utf-8" ?>
<plugin xmlns="xmlns://www.fortify.com/schema/pluginmanifest-1.0.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="PluginDescriptor"
        id="com.fortify.ssc.parser.tenable.io.cs" api-version="1.0">
    <plugin-info>
        <name>Tenable.io Container Security parser plugin</name>
        <version><!--VERSION-->0.0<!--/VERSION--></version>
        <data-version>1</data-version>
        <vendor name="Micro Focus" url="https://www.microfocus.com"/>
        <description>Tenable.io Container Security parser plugin</description>
        <resources>
            <localization>
                <language id="default" location="/resources/tenable-io-cs_en.properties"/>
                <language id="en" location="/resources/tenable-io-cs_en.properties"/>
            </localization>
            <images>
                <image imageType="icon" location="/images/tenable-io-cs-icon.png"/>
                <image imageType="logo" location="/images/tenable-io-cs-logo.png"/>
            </images>
        </resources>
    </plugin-info>
    <issue-parser xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <engine-type>TENABLE_IO_CS</engine-type>
        <supported-engine-versions>[5.0, 5.2.2]</supported-engine-versions>
        <view-template location="/viewtemplate/TenableIoCsParserPlugin.json">
            <description>Default Tenable.io Container Security view template.</description>
        </view-template>
    </issue-parser>
</plugin>